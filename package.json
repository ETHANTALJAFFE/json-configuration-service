{
  "name": "json-configuration-service",
  "version": "0.1.0",
  "main": "server/index.js",
  "scripts": {
    "start:client:dev": "npm run --prefix editor start",
    "build:client": "npm run --prefix editor build",
    "start:server": "node -r esm server/index.js",
    "start:dev": "concurrently \"npm run start:server\"  \"npm run start:client:dev\"",
    "start": "concurrently \"npm run build:client\" \"npm run start:server\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ETHANTALJAFFE/json-configuration-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ETHANTALJAFFE/json-configuration-service/issues"
  },
  "homepage": "https://github.com/ETHANTALJAFFE/json-configuration-service#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^6.11.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "http-errors": "^1.7.3",
    "json-configurator-store": "^0.1.2",
    "prettier": "^2.0.5",
    "swagger-ui-express": "^4.1.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1"
  },
  "description": ""
}
